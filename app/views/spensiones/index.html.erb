
<div class="card">
  <div class="card-header" > Cuadro Resúmen x AFP </div>
  <div class="card-body">
    <nav class="navbar navbar-expand-lg bg-light">
      <div class="container-fluid">
        <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-table" viewBox="0 0 16 16">
            <path d="M0 2a2 2 0 0 1 2-2h12a2 2 0 0 1 2 2v12a2 2 0 0 1-2 2H2a2 2 0 0 1-2-2V2zm15 2h-4v3h4V4zm0 4h-4v3h4V8zm0 4h-4v3h3a1 1 0 0 0 1-1v-2zm-5 3v-3H6v3h4zm-5 0v-3H1v2a1 1 0 0 0 1 1h3zm-4-4h4V8H1v3zm0-4h4V4H1v3zm5-3v3h4V4H6zm4 4H6v3h4V8z"/>
          </svg>
          <i class="bi bi-table"></i>
        <a class="navbar-brand" href="#">Cuadro Resúmen</a>
        <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
          <span class="navbar-toggler-icon"></span>
        </button>
        <div class="collapse navbar-collapse" id="navbarSupportedContent">
          <ul class="navbar-nav me-auto mb-2 mb-lg-0">
           
         
          </ul>
          <form class="d-flex" role="search">
            <input class="form-control me-2" type="search" placeholder="Búsqueda" aria-label="Search">
            <button class="btn btn-outline-success" type="submit">Búsqueda</button>
          </form>
        </div>
      </div>
    </nav>

    <table class="table table-striped ">
    <thead >
        <tr>
        <th>Fecha</th>
        <th>CAPITAL</th>
        <th>CUPRUM</th>
        <th>HABITAT</th>
        <th>MODELO</th>
        <th>PLANVITAL</th>
        <th>PROVIDA</th>
        <th>UNO</th>
        </tr>
    </thead>

    <% if @data.count > 0 %>


    <%  i=0 %>

    <%@csv_file_path.each_line do |line|%>
        <% splited = line.split(';') %>
            <%if i >  4 %>
                <tr>
                    <td><%= splited[0]  %></td>
                    <td><%= splited[1]  %></td>
                    <td><%= splited[3]  %></td>
                    <td><%= splited[5]  %></td>
                    <td><%= splited[7]  %></td>
                    <td><%= splited[9]  %></td>
                    <td><%= splited[11]  %></td>
                    <td><%= splited[13]  %></td>
                </tr>
            <%end%> 
            <% i=i+1 %>
    <%end%> 


    </table>


    <%else%>
    <h1>No hay resultados para la búsqueda </h1>
    <%end%>

  </div>
</div>


